<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
               
     <context:annotation-config/>
     
  
 
 <bean class= "com.beanlifecycle.Car" name="c1" init-method="init" destroy-method="destroy">
 <property name="price" value="800000"></property>
 </bean>
 
 
 <bean class= "com.beanlifecycle.First" name="e1">
 <property name="subject" value="maruti"></property>
 </bean>
 

 </beans>